
<%- include('./header') %>
<% if(locals.flash && locals.flash.error) { %>
    <div class="alert alert-success">
      <strong>Success!</strong> <%= flash.error %>
    </div>
    <% } %>
<table class="table table-dark">
    <thead class="thead-dark">
        <tr>
            <th scope="col">Message</th>
            <th scope="col">Date</th>
            <th scope="col">Action</th>
        </tr>
    </thead>
    <% objets.forEach(function(item) { %>
        <tbody>
            <tr>
                <td><%= item.contenu %></td>
                <td><%= item.date %></td>
                <td><a href="/remove/<%= item.id %>" class="btn btn-danger" >Supprimer</a><a href="/update/<%= item.id %>" class="btn btn-warning">Modifier</a></td>
            </tr>
        </tbody>
    <% }); %>
</table>
<%- include('./footer') %>
